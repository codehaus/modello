<model>
  <id>tissue</id>
  <name>Tissue</name>
  <description>Tissue's entity model.></description>
  <defaults>
    <default>
      <key>package</key>
      <value>org.mergere.tissue</value>
    </default>
  </defaults>
  <classes>
    <class stash.storable="true">
      <name>Issue</name>
      <version>1.0.0</version>
      <fields>
        <field>
          <name>id</name>
          <required>true</required>
          <version>1.0.0</version>
          <type>String</type>
        </field>
        <!-- This field should get persistence-modifier="none" -->
        <field stash.storable="false">
          <name>summary</name>
          <version>1.0.0</version>
          <type>String</type>
        </field>
        <field>
          <name>assignee</name>
          <version>1.0.0</version>
          <!-- this should set default-fetch-group=true -->
          <association stash.part="false">
            <type>User</type>
          </association>
        </field>
        <field>
          <name>reporter</name>
          <version>1.0.0</version>
          <!-- this should not set default-fetch-group at all -->
          <association>
            <type>User</type>
          </association>
        </field>
        <field>
          <name>status</name>
          <version>1.0.0</version>
          <type>String</type>
        </field>
        <field>
          <name>project</name>
          <version>1.0.0</version>
          <association>
            <type>Project</type>
          </association>
        </field>
        <field>
          <name>configuration</name>
          <version>1.0.0</version>
          <type>Map</type>
          <!-- this should set default-fetch-group=true -->
          <!-- The keyType will once in the future be properly validated and then this
               should fail -->
          <association stash.part="true" stash.keyType="Foo">
            <type>String</type>
            <multiplicity>*</multiplicity>
          </association>
        </field>
        <field>
          <name>configuration2</name>
          <version>1.0.0</version>
          <type>Map</type>
          <!-- this should not set default-fetch-group=true -->
          <!-- The keyType will once in the future be properly validated and then this
               should fail -->
          <association stash.keyType="Bar">
            <type>String</type>
            <multiplicity>*</multiplicity>
          </association>
        </field>
      </fields>
    </class>

    <!--
     |
     |
     |
    -->
    <class stash.storable="true">
      <name>Project</name>
