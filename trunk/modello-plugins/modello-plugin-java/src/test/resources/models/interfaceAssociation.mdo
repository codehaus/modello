<model>
  <id>interface-association-test</id>
  <name>InterfaceAssociationTest</name>

  <defaults>
    <default>
      <key>package</key>
      <value>org.codehaus.modello.ifaceassociation.package1</value>
    </default>
  </defaults>

  <classes>
    <class java.clone="deep">
      <name>Location</name>
      <fields>
