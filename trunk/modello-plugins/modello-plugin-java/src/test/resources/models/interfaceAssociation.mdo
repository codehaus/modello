            <multiplicity>*</multiplicity>
          </association>
        </field>
        <field>
          <name>relatives</name>
          <type>Set</type>
          <association java.useInterface="IPerson">
            <type>Person</type>
            <multiplicity>*</multiplicity>
          </association>
        </field>
        <field>
          <name>mother</name>
          <association java.useInterface="IPerson">
            <type>Person</type>
          </association>
        </field>
      </fields>
    </class>

    <class java.clone="deep">
      <name>Person</name>
      <interfaces>
        <interface>IPerson</interface>
      </interfaces>
    </class>
  </classes>

</model>